{
    "manifest_version": 2,
    "name": "Jawaker Hack",
    "description": "",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": [
                "https://www.jawaker.com/*"
            ],
            "js": [
                "js/content.js"
            ],
            "css": [
                "css/main.css"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "tabs",
        "contextMenus",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}